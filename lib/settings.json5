  "vetur.validation.template": false,
  "vsicons.dontShowNewVersionMessage": true,
  "window.title": "${activeEditorMedium}${separator}${rootPath}",
  "window.restoreWindows": "all",
  "workbench.iconTheme": "vscode-icons",
